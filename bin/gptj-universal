#! /usr/bin/env python3
from GPTJ.Basic_api import SimpleCompletion
import sys, getopt

prompt = ""
for line in sys.stdin:
    prompt = prompt + line
print(prompt)
max_length = 100
temperature = 0.09 #0.09
top_probability = 1.0
query = SimpleCompletion(prompt, length=max_length, t=temperature, top=top_probability)
Query = query.simple_completion()
print(Query)
